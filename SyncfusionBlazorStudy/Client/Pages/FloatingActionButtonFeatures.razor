@page "/floatingactionbutton-features"
@using Syncfusion.Blazor
@using Syncfusion.Blazor.Buttons
<PageTitle>Floating Action Button</PageTitle>
<div id="target" style="position:relative;height:50vh">
        <h2>Floating Action Button</h2>
</div>
<SfFab IconCss="fab-icons fab-icon-people" Content="Contact" Position=FabPosition.BottomRight Target="#target"></SfFab>
<br/>
<div>
  <h3>Selected Features:</h3>
    <ul class="ulstyle">
      <li class="list"> Default</li>
      <li class="list"> Theme - Fluent</li>
    </ul>
</div>
<br/>
<style>
    @@font-face {
        font-family: 'fab-icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }
	.e-fab.e-btn.e-fab-bottom {
            bottom: 0px;
        }
    [class^="fab-icon-"],
    [class*=" fab-icon-"] {
        font-family: 'fab-icons' !important;
        speak: none;
        font-size: 55px;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: 1;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }
    .fab-icon-people:before {
        content: "\e70a";
    }
	.ulstyle {
        margin: 0px;
        padding-left: 20px;
        display: inline-block;
    }
    .list {
    float: left;
    line-height: 20px;
    margin: 10px;
    min-width: 250px;
    }
</style>
